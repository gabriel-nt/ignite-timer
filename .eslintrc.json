{
  "extends": "@rocketseat/eslint-config/react",
   "rules": {
    "no-unused-vars": "off",
    "prettier/prettier": ["error", {
      "printWidth": 80,
      "tabWidth": 2,
      "endOfLine": "auto",
      "singleQuote": true,
      "trailingComma": "all",
      "arrowParens": "always",
      "semi": false
    }]
  }
}
